{
    "name": "vueberry",
    "version": "0.0.1",
    "homepage": "https://vueberry.ru",
    "description": "Lightweight UI components for Vue.js",
    "author": "Shamil Tepsurkaev <ya.sham@yandex.ru>",
    "main": "./dist/vueberry.umd.js",
    "module": "./dist/vueberry.es.js",
    "exports": {
        ".": {
            "import": "./dist/vueberry.es.js",
            "require": "./dist/vueberry.umd.js"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "files": [
        "dist",
        "src"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/sham-soft/vueberry.git"
    },
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check build-only",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/"
    },
    "keywords": [
        "vueberry",
        "vue",
        "vuejs",
        "components"
    ],
    "dependencies": {
        "vue": "^3.2.47"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.2.0",
        "@types/node": "^18.14.2",
        "@vitejs/plugin-vue": "^4.0.0",
        "@volar/vue-language-plugin-pug": "^1.2.0",
        "@vue/eslint-config-prettier": "^7.1.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "@vue/tsconfig": "^0.1.3",
        "eslint": "^8.34.0",
        "eslint-plugin-vue": "^9.9.0",
        "eslint-plugin-vue-pug": "^0.6.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.4",
        "pug": "^3.0.2",
        "sass": "^1.59.2",
        "typescript": "~4.8.4",
        "vite": "^4.1.4",
        "vue-tsc": "^1.2.0",
        "normalize.css": "^8.0.1",
        "pinia": "^2.0.32",
        "vue-router": "^4.1.6"
    }
}
